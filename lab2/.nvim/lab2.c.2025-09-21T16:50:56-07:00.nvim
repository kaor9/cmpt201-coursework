#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <unistd.h>
#include <sys/wait.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char* input = NULL;
  size_t len =0;

  pid_t pid = fork(); 

  printf("Enter programs to run.\n");
  if (getline(&input, &len, stdin) != -1) {
    if (pid == 0) {
      printf("%s", input);
    }
  } 
  else {
    printf("Error on reading input, closing program\n");
    return 1;
  }


  return 0;
}
